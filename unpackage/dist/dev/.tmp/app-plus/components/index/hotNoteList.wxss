
.flex-item-V {
	width: 100%;
	width: 100%;
	height: 70px;
	line-height: 70px;
	margin-top: 0PX;
	border-radius: 0PX;
	background-color: #fff0cd;
}
.note-img-view{
	width: 20%;
	display: inline-block;
	margin-top: 10px;
}
.note-content{
   width: 80%;
   display: inline-block;
}
.note-img{
	height: 50px;
	width: 50px;
	border-radius: 50px;
}
.note-list{
	border-bottom: 1px solid #d6d7d8;
}
.note-title{
  font-size: 12px;
  height: 45px;
  line-height: 45px;
  overflow: hidden;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
  white-space: nowrap;
}
.note-information{
  height: 15px;
}
.note-information-text{
  font-size: 10px;
  width: 32%;
  display:inline-block;
}
.note-information-text:first-child{
	text-align: left;
}
.note-information-text:nth-child(2){
	text-align: center;
}
.note-information-text:nth-child(3){
	text-align: right;
	margin-right:10px;
}

