
.top-warp{
	z-index: 9990;
	position: fixed;
	top: --window-top; /* css变量 */
	left: 0;
	width: 100%;
	height: 120rpx;
	background-color: white;
}
.top-warp .tip{
	font-size: 28rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
}
.top-warp .nav{
	text-align: center;
	height: 60rpx;
	border-bottom: 1rpx solid #ddd;
}
.top-warp .nav view{
	display: inline-block;
	width: 22%;
	line-height: 60rpx;
	font-size: 28rpx;
}
.top-warp .nav .active{
	border-bottom: 2rpx solid #FF6990;
	color: #FF6990;
}

