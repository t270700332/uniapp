
page {
	background-color: #fff6df;
}
.top-warp{
	z-index: 9990;
	position: fixed;
	top: 0; /* css变量 */
	left: 0;
	width: 100%;
	height: 60rpx;
	background-color: #fff6df;
}
.top-warp .nav{
	text-align: center;
	height: 60rpx;
	border-bottom: 1rpx solid #ddd;
}
.top-warp .nav view{
	display: inline-block;
	width: 22%;
	line-height: 60rpx;
	font-size: 28rpx;
}
.top-warp .nav .active{
	border-bottom: 2rpx solid #591804;
	color: #591804;
}



